{
  "name": "spaceport",
  "version": "0.0.1",
  "description": "Threadpooling for React.js providers / React-Redux",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build && rimraf lib",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/secberus-oss/Spaceport.git"
  },
  "keywords": [
    "thread",
    "worker",
    "thread-pool",
    "redux",
    "multithreading"
  ],
  "author": "Secberus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/secberus-oss/Spaceport/issues"
  },
  "homepage": "https://github.com/secberus-oss/Spaceport#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "inquirer": "^7.2.0",
    "npm": "^6.14.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
