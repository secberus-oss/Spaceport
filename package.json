{
  "name": "spaceport",
  "version": "0.0.1",
  "description": "Threadpooling for React.js providers / React-Redux",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build && rimraf lib",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/secberus-oss/Spaceport.git"
  },
  "keywords": [
    "thread",
    "worker",
    "thread-pool",
    "redux",
    "multithreading"
  ],
  "author": "Secberus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/secberus-oss/Spaceport/issues"
  },
  "homepage": "https://github.com/secberus-oss/Spaceport#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@types/react": "^16.9.38",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "inquirer": "^7.2.0",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "npm": "^6.14.5",
    "prettier": "^2.0.5",
    "redux": "^4.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {},
  "peerDependencies": {
    "redux": "~4.0.5"
  },
  "lint-staged": {
    "*.{js,ts;}": [
      "eslint --fix"
    ]
  }
}
