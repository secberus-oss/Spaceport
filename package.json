{
  "name": "spaceportjs",
  "version": "0.0.2",
  "description": "Componentized threadpooling to execute groups of actions.",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf build && rimraf lib",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --ignore '**/*.test.d.ts,**/*.test.ts,**/*.test.js' --source-maps inline",
    "bundle": "rollup -c",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/secberus-oss/Spaceport.git"
  },
  "keywords": [
    "thread",
    "worker",
    "thread-pool",
    "redux",
    "multithreading"
  ],
  "author": "Secberus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/secberus-oss/Spaceport/issues"
  },
  "homepage": "https://github.com/secberus-oss/Spaceport#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^26.0.7",
    "@types/lodash": "^4.14.158",
    "@types/node": "^14.0.23",
    "@types/rc": "^1.1.0",
    "@types/react": "^16.9.38",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "inquirer": "^7.2.0",
    "jest": "^26.1.0",
    "jsdom": "^16.4.0",
    "jsdom-worker": "^0.2.1",
    "lint-staged": "^10.2.11",
    "node-fetch": "^2.6.1",
    "npm": "^6.14.7",
    "prettier": "^2.0.5",
    "redux": "^4.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "lodash": "^4.17.19",
    "promise-worker": "^2.0.1",
    "uuid": "^8.3.0"
  },
  "peerDependencies": {},
  "lint-staged": {
    "*.{js,ts;}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  }
}
